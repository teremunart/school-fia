<!ELEMENT kursprogramm (kurs+)>
<!ELEMENT kurs (bezeichnung, referententeam)>
<!ATTLIST kurs id ID #REQUIRED kurstyp CDATA #IMPLIED>
<!ELEMENT bezeichnung (#PCDATA)>
<!ELEMENT referententeam (referent+)>
